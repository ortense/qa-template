  "bugs": {
    "url": "https://github.com/ortense/qa-template/issues"
  },
  "homepage": "https://github.com/ortense/qa-template#readme",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-latest": "^6.16.0",
    "chai": "^3.5.0",
    "joi": "^10.2.0",
    "joi-assert": "0.0.3",
    "mocha": "^3.2.0",
    "mochawesome": "^2.0.2",
    "supertest": "^2.0.1"
